#include <bits/stdc++.h>
using namespace std;
int main()
{
    int T;
    cin >> T;
    while (T--)
    {
        int n;
        cin>>n;
        int arr[n];
        int a=n*(n+1)/2;
        
        if(a%n==0)cout<<-1<<endl;
        else
        {
            for(int i=2;i<=n;i+=2)cout<<i<<" "<<i-1<<" ";
            cout<<endl;
        }
    }
    return 0;
}
